// Java

class Solution {
    public String solution(int n) {
        String answer = "";
        for (int i=1; i<=n; i++){
            if (i%2 == 1){
                answer += "수";
            }else{
                answer +="박";
            }
            
        }
        return answer;
    }
}

// Python

def solution(n):
    
    answer = ''
    for i in range(n):
        if i % 2 == 0:
            answer += '수'
        else:
            answer += '박'
            
            
    return answer
