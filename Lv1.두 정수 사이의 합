// java

public class IntegerCoding {
	
	public long solution(int a, int b) {
		
		long answer = 0;
		
		if (a <= b) {
			for (int i=a; i<=b; i++) {
				answer += i;
			}
		} else {
			for (int i=b; i<=a; i++) {
				answer += i;
			}
		}
		
		return answer;
		
	}
	
	public static void main(String[] args) {
		
		IntegerCoding i = new IntegerCoding();
		
		System.out.println(i.solution(3, 5));

	

	}

}
