// java

public class IntegerCoding {
	
	public long solution(int a, int b) {
		
		long answer = 0;
		
		for (int i=a; i<=b; i++) {
			answer += i;
		}
		
		return answer;
		
	}
	
	public static void main(String[] args) {
		
		IntegerCoding i = new IntegerCoding();
		
		System.out.println(i.solution(3, 5));

	

	}

}
